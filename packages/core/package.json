{
  "name": "@stencil-storybook-boilerplate/core",
  "private": true,
  "version": "0.0.1",
  "description": "Stencil Component Starter",
  "author": "Artur Sopelnik <artur.sopelnik93@gmail.com>",
  "main": "dist/index.cjs.js",
  "module": "dist/index.js",
  "es2015": "dist/esm/index.js",
  "es2017": "dist/esm/index.js",
  "types": "dist/types/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/core/core.esm.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/ionic-team/stencil-component-starter.git"
  },
  "files": [
    "dist/",
    "loader/"
  ],
  "scripts": {
    "build": "stencil build --docs",
    "start": "stencil build --docs --watch",
    "test": "npm run test.spec && npm run test.e2e",
    "test.spec": "stencil test --spec",
    "test.e2e": "playwright test",
    "generate": "stencil generate",
    "update": "npx npm-check-updates -u",
    "preview": "npx http-server www"
  },
  "devDependencies": {
    "@playwright/test": "^1.50.1",
    "@stencil/angular-output-target": "^0.10.2",
    "@stencil/core": "4.27.1",
    "@stencil/playwright": "^0.2.1",
    "@stencil/react-output-target": "^0.8.2",
    "@stencil/sass": "3.0.12",
    "@stencil/vue-output-target": "^0.9.5",
    "@types/jest": "^29.5.14",
    "http-server": "^14.1.1",
    "jest": "^29.7.0",
    "jest-cli": "^29.7.0"
  },
  "license": "MIT"
}
