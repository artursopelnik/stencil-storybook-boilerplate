      "body-text": { "type": "color", "value": "{global.color.black}" }
    },
    "spacing": {
      "static": {
        "small": { "type": "dimension", "value": "{global.size.8}" },
        "medium": { "type": "dimension", "value": "{global.size.16}" },
        "large": { "type": "dimension", "value": "{global.size.32}" }
      },
      "fluid": {
        "small": {
          "type": "dimension",
          "value": "clamp({global.size.8}, {global.size.4} + 1vw, {global.size.16})"
        },
        "medium": {
          "type": "dimension",
          "value": "clamp({global.size.16}, {global.size.12} + 1.25vw, {global.size.28})"
        },
        "large": {
          "type": "dimension",
          "value": "clamp({global.size.32}, {global.size.20} + 2vw, {global.size.40})"
        }
      }
    }
  },
  "component": {
    "button": {
      "medium": {
        "font-size": { "type": "dimension", "value": "{global.size.16}" }
      }
    }
